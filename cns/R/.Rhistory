W <- function(s) {
neg_ind <- s<=0
pos_ind <- s>0
if(length(neg_ind>0)) {
s[neg_ind] <- exp(s[neg_ind]/t_syn)*(Apos*(1-s[neg_ind]/t_pos_) + Aneg*(1-s[neg_ind]/t_neg_))
}
if(length(pos_ind>0)) {
s[pos_ind] <- Apos*exp(-s[pos_ind]/t_pos) + Aneg*exp(-s[pos_ind]/t_neg)
}
return(nu*s)
}
