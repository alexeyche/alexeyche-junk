alpha <- 0.45
beta <- 0.55
tr <- -50 # mV
g <- Vectorize(function(u) {
(beta/alpha)*(log(1+exp(alpha*(tr-u))) -alpha*(tr-u))
})
p_stroke <- function(u) {
beta/(1+exp(alpha*(tr-u)))
}
