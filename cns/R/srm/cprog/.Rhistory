gen_pattern = function(f, dur, N, len=1000, del=75) {
net=blank_net(N)
for(t in seq(0,dur,length.out=len)) {
i = as.integer(99*(1+f(t/del))/2)+1
net[[i]] = c(net[[i]], t)
}
return(net)
}
