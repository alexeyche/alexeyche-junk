{
    "language"        : "PYTHON",
    "main-file"       : "unsup_snn",
    "experiment-name" : "unsupervised_clustering",
    "variables": {
        "weight_decay": {
            "type": "FLOAT",
            "size": 1,
            "min":  0.005,
            "max":  0.1
        },
        "tau_c": {
            "type": "FLOAT",
            "size": 1,
            "min":  50,
            "max":  300
        },
        "beta": {
            "type": "FLOAT",
            "size": 1,
            "min":  0.5,
            "max":  5.0
        }
    },
    "tasks": {
      "cluster_criterion" : {
          "type"        : "OBJECTIVE",
          "likelihood"  : "GAUSSIAN"
      }
    },
    "polling-time" : 5
}

