plot_tsne <- function(data,target) {
require(tsne)
colors = rainbow(length(unique(target)))
names(colors) =  unique(target)
ecb = function(x,y){ plot(x,t='n'); text(x,labels=target, col=colors[target]); Sys.sleep(0.5); }
tsne_dbn = tsne(data, epoch_callback = ecb, perplexity=50)
}
